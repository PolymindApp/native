{
  "expo": {
    "name": "polymind-native",
    "slug": "polymind-native",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
	"sdkVersion": "37.0.0",
	"android": {
	  "package": "com.vue.polymindnative",
	  "versionCode": 1
	},
    "ios": {
	  "bundleIdentifier": "com.vue.polymindnative",
	  "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "json",
        "ts",
        "tsx",
        "vue"
      ]
    }
  }
}
